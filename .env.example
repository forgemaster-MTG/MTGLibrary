# Copy this file to .env and edit the values for your environment
# Example DATABASE_URL for Postgres running on NAS Docker
# Replace <user>, <password>, <nas-ip>, <port>, <database> with your values
# DATABASE_URL=postgres://<user>:<password>@<nas-ip>:<port>/<database>

# Or set PGHOST/PGUSER/PGPASSWORD/PGDATABASE instead of DATABASE_URL
# PGHOST=
# PGUSER=postgres
# PGPASSWORD=change_me
# PGDATABASE=mtglibrary_dev

# Server port for Express API
PORT=3000
