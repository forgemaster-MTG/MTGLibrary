    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "zod": "3.24.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:server": "node -r dotenv/config server/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate": "knex migrate:latest --knexfile knexfile.cjs",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.cjs",
    "import-precons": "node scripts/import-precons-to-pg.js",
    "import-users": "node scripts/import-users-to-pg.js",
    "electron:dev": "wait-on http://localhost:5173 && electron .",
    "electron:build": "vite build && electron-builder"
  },
  "build": {
    "appId": "com.mtgforge.app",
    "productName": "MTG Forge",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "!**/node_modules/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "npmRebuild": false,
    "extraMetadata": {
      "dependencies": {}
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "jsdom": "^28.0.0",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  }
}